{
  "name": "suneditor-vue",
  "description": "A Vue 3 Component for Suneditor",
  "type": "module",
  "author": "kazettique",
  "version": "0.0.1",
  "private": false,
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/suneditor-vue.umd.cjs",
  "module": "dist/suneditor-vue.js",
  "browser": {
    "./styles.css": "./dist/style.css"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*.css": {
      "import": "./dist/*.css",
      "require": "./dist/*.css"
    }
  },
  "style": "dist/style.css",
  "types": "./dist/SunEditor.vue.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint:fix": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "suneditor": "^2.44.3",
    "vue": "^3.2.47"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^18.11.18",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitest/ui": "^0.28.5",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.10",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-sort-keys": "^2.3.5",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^21.1.0",
    "minimist": "^1.2.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "rollup-plugin-typescript2": "^0.34.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vitest": "^0.28.5",
    "vue-tsc": "^1.1.0"
  },
  "keywords": [
    "editor",
    "wysiwyg",
    "suneditor",
    "text-editor",
    "word",
    "vue",
    "vue3",
    "typescript"
  ]
}